<h1>Search for Coffee Shops Near Me</h1>
<%= form_tag '/search' do %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>
  <%= submit_tag "Search!" %>
<% end %>
<div>
  <% if @error %>
    <%= @error %>
  <% end %>
  <% if @venues %>
    <% @venues.each do |v| %>
      <%= v['name'] %> <%= v["location"]["address"] %>
      <br><br>
    <% end %>
  <% end %>
</div>
